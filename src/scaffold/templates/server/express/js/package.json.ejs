{
  "name": "<%= name %>",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "node esbuild.config.js",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate"
  },

  "dependencies": {
    "dotenv": "latest",
    "express": "latest"
    <% if (orm === 'mongoose') { %>,
    "mongoose": "latest"
    <% } else if (orm === 'prisma') { %>,
    "prisma": "latest",
    "@prisma/client": "latest"
    <% } %>
  },

  "devDependencies": {
    "nodemon": "latest",
    "esbuild": "latest"
  }
}
